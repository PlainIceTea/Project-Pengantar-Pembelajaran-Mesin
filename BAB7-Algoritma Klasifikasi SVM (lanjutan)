      "source": [
        "label_uji_biner = label_uji.map({'Setosa': -1, 'Versicolor': -1, 'Virginica': 1})\n",
        "prediksi_biner = [1 if kelas in prediksi else -1 for kelas in label_uji_biner]\n",
        "\n",
        "jumlah_benar = sum(1 for pred, true in zip(prediksi_biner, label_uji_biner) if pred == true)\n",
        "print(\"Jumlah data latih yang berhasil diprediksi dengan benar:\", jumlah_benar)"
